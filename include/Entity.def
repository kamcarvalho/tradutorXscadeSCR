    "<pragmas>" "<ed:Variable" "oid=\"!ed/" PragmaId "/" PragmaId "/" INT "\"/>" "</pragmas>"                                   -> Pragma   {cons("Pragma3")}
    "<pragmas>" "<ed:Variable" "oid=\"!ed/" PragmaId "/" PragmaId "\"" "/>" "</pragmas>"                                        -> Pragma   {cons("Pragma4"), avoid}

